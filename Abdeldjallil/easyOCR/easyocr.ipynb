    "import easyocr\n",
    "\n",
    "# Create an EasyOCR reader for Arabic\n",
    "reader = easyocr.Reader(['ar'])\n",
    "\n",
    "# Path to the image file\n",
    "image_path = 'short_story-018.png'\n",
    "\n",
    "# Perform OCR\n",
    "results = reader.readtext(image_path)\n",
    "\n",
    "# Display the results\n",
    "for result in results:\n",
    "    print(result[1])\n"
   ]
